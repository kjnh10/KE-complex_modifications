{
  "title": "Personal rules (@kjnh10)",
  "rules": [
    {
    "description": "Misc setting (rev 1)",
      "manipulators": [<%#{{{%>
        <%# C-; => Escape %>
        {
          "type": "basic",
          "from": <%= from("semicolon", ["control"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["escape"]]) %>
        },
        <%# C-: => hyphen %>
        {
          "type": "basic",
          "from": <%= from("quote", ["control"], ["caps_lock"]) %>,
          "to": <%= to([["hyphen"]]) %>
        }
      ]<%#}}}%>
    }
  ]
}
